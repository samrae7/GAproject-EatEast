<div class='page-wrapper'>

  <div id="business-form"></div>

  <div id="business-listing"></div>

  <div id='map-box'>
    <div class="row">
      <div class="small-12 large-4 large-offset-1 columns">
        <ul class="pricing-table reviews-table">
          <li class="title">Reviews</li>
        <!--   <li class="description">An awesome description</li> -->
          <li class="bullet-item">Roof party tofu fingerstache, Banksy Odd Future twee craft beer seitan whatever raw denim freegan. Locavore viral readymade, quinoa tofu bespoke fanny pack messenger bag Thundercats raw denim roof party bitters. PBR&B drinking vinegar tousled banjo pickled pop-up, taxidermy skateboard mustache bitters scenester. </li>
        </ul>
      </div>
      <div class="small-12 large-5 columns end">
        <div id='map'></div>
      </div>
    </div>
  </div>

  <div id='businesses-list'>

    <ul class="small-block-grid-1 medium-block-grid-2 large-block-grid-3">

      <% @businesses.each do |business| %>

      <li>
        <div class='index-listing'>
        <div class='image-container'>
          <!-- <img class='business-image-show' src="<%#= business.parseImageUrl %>"> -->
          <%= link_to image_tag(business.parseImageUrl, :class => "business-image-show") +'some text', business, remote:true, method: :get %>
        </div>
        <!--     <%#= image_tag(business.parseImageUrl, :class => "business-image-show") %> -->

        <!-- link helper that also assigns an individual id to each link on the page -->
        <div class='text-container'>
        <%= link_to business.name, business, id:'business'+business.id.to_s, remote:true, method: :get %>
        </div>
      </li>

       <% end %>
   
    </ul>

  </div>

</div>





